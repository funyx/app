{
    "name": "funyx/app",
    "description": "laravel-atk4",
    "keywords": ["framework", "laravel", "atk4", "console", "cli"],
    "version": "0.0.1",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "funyx",
            "email": "ddobchev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "atk4/ui": "dev-develop as 3.0.0",
        "laminas/laminas-text": "^2.8",
        "laravel/framework": "^8.44"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["atk4"],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:funyx/data.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:funyx/ui.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:funyx/login.git"
        }
    ]
}
